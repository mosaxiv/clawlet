{
  "agents": {
    "defaults": {
      "model": "openrouter/google/gemini-3-flash-preview",
      "maxTokens": 8192,
      "temperature": 0.2,
      "memoryWindow": 50,
      "memorySearch": {
        "enabled": false,
        "provider": "openai",
        "model": "text-embedding-3-small",
        "remote": {
          "baseURL": "https://api.openai.com/v1",
          "apiKey": "",
          "headers": {}
        },
        "store": {
          "path": "./memory-store",
          "vector": {
            "enabled": true
          }
        },
        "chunking": {
          "tokens": 400,
          "overlap": 80
        },
        "query": {
          "maxResults": 6,
          "minScore": 0.35,
          "hybrid": {
            "vectorWeight": 0.7,
            "textWeight": 0.3,
            "candidateMultiplier": 4
          }
        },
        "cache": {
          "enabled": true,
          "maxEntries": 1000
        },
        "sync": {
          "onSearch": true
        }
      }
    }
  },
  "tools": {
    "restrictToWorkspace": true,
    "exec": {
      "timeoutSec": 60
    },
    "web": {
      "braveApiKey": ""
    }
  },
  "cron": {
    "enabled": true
  },
  "heartbeat": {
    "enabled": true,
    "intervalSec": 1800
  },
  "gateway": {
    "listen": "0.0.0.0:18790"
  },
  "channels": {
    "discord": {
      "enabled": false,
      "token": "",
      "allowFrom": [
        "123456789"
      ],
      "gatewayURL": "wss://gateway.discord.gg/?v=10&encoding=json",
      "intents": 37377
    },
    "slack": {
      "enabled": false,
      "allowFrom": [
        "U123ABC",
        "U456DEF"
      ],
      "botToken": "xoxb-...",
      "appToken": "xapp-...",
      "groupPolicy": "mention",
      "groupAllowFrom": [
        "C123CHAN"
      ],
      "dm": {
        "enabled": true
      }
    },
    "telegram": {
      "enabled": false,
      "token": "",
      "allowFrom": [
        "123456789"
      ],
      "baseURL": "https://api.telegram.org",
      "pollTimeoutSec": 25,
      "workers": 2
    },
    "whatsapp": {
      "enabled": false,
      "allowFrom": [
        "+1234567890"
      ],
      "sessionStorePath": "./whatsapp-session.db"
    }
  }
}